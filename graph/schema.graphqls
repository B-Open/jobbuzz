# GraphQL schema example
#
# https://gqlgen.com/getting-started/

input NewUser {
  email: String!
  password: String!
}

type User {
  id: Int!
  email: String!
}

type Job {
  id: Int!
  createdAt: String!
  updatedAt: String!
  title: String!
  company: String!
  salary: String!
  location: String!
}

type Query {
  jobs: [Job!]!
}

type Mutation {
  registerAccount(input: NewUser!): User!
}
